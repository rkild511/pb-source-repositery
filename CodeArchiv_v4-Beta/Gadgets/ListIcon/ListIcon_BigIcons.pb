; English forum: http://www.purebasic.fr/english/viewtopic.php?t=9559&highlight=
; Author: Denis (updated for PB 4.00 by Andre)
; Date: 20. February 2004
; OS: Windows
; Demo: No


; Here is an example with bigger images in the listicon.
; This example works only with icon format, If you use Bitmap,
; you must use ImageList_Add API instead ImageList_AddIcon.
; Read the MS doc about ImageList.


Enumeration
  #MainWindow
  #ListIconGadget1
  #Image
EndEnumeration

; ***************************************************************************

Procedure AddItem(Gadget.l, Row.l, Column.l, Text$, ImageIndex.l)
  ; add an item with an image from the ListImage or a subitem without image
  ; put -1 to ImageIndex to not display Image
  ; fill up var to set subitem icon and text
  var.lvitem

  Var\mask = #LVIF_IMAGE | #LVIF_TEXT
  Var\iItem = Row         ; row number
  Var\iSubItem = Column   ; subitem
  Var\pszText = @Text$    ; text to set
  Var\iImage = ImageIndex ; index of icon in the list

  ; set text + icon in the listicongadget item/subitem
  If column
    SendMessage_(GadgetID(Gadget), #LVM_SETITEM, 0, @Var)
  Else
    SendMessage_(GadgetID(Gadget), #LVM_INSERTITEM, 0, @Var)
  EndIf
EndProcedure

; ***************************************************************************

If OpenWindow(#MainWindow, 0, 0, 420, 300, "", #PB_Window_ScreenCentered | #PB_Window_SystemMenu)
  If CreateGadgetList(WindowID(#MainWindow)) And ListIconGadget(#ListIconGadget1, 10, 55, 400, 236, "Column 1", 398 / 5 + 70, #PB_ListIcon_MultiSelect | #PB_ListIcon_FullRowSelect )
    AddGadgetColumn(#ListIconGadget1, 1, "Column 2", 398 / 5)
    AddGadgetColumn(#ListIconGadget1, 2, "Column 3", 398 / 5)
    AddGadgetColumn(#ListIconGadget1, 3, "Column 4", 398 / 5)

    ; create an Image List : first and second param are icon size
    Hwnd_ListSmall = ImageList_Create_(100, 60, #ILC_MASK | #ILC_COLOR32, 0, 30)

    ; assign the image list to the ListIconGadget
    SendMessage_(GadgetID(#ListIconGadget1), #LVM_SETIMAGELIST, #LVSIL_SMALL, Hwnd_ListSmall)

    TransparentIco.l = CatchImage(#Image, ?TransparentIco)
    IndexTransparentIco = ImageList_AddIcon_(Hwnd_ListSmall, TransparentIco)
    FreeImage(#Image)

    ArrowL.l = CatchImage(#Image, ?FlecheGIco)
    IndexArrowL = ImageList_AddIcon_(Hwnd_ListSmall, ArrowL)
    FreeImage(#Image) ; you can destroy the image because there is a copy inside the list

    ArrowR.l = CatchImage(#Image, ?FlecheDIco)
    IndexArrowR = ImageList_AddIcon_(Hwnd_ListSmall, ArrowR)
    FreeImage(#Image)

    CroixB.l = CatchImage(#Image, ?CroixBleueIco)
    IndexCroixB = ImageList_AddIcon_(Hwnd_ListSmall, CroixB)
    FreeImage(#Image)

    PBIco.l = CatchImage(#Image, ?PB_Ico)
    IndexPBIco = ImageList_AddIcon_(Hwnd_ListSmall, PBIco)
    FreeImage(#Image)

    Number4Ico.l = CatchImage(#Image, ?Number_4Ico)
    IndexNumber4Ico = ImageList_AddIcon_(Hwnd_ListSmall, Number4Ico)
    FreeImage(#Image)

    ; add items with images
    AddItem(#ListIconGadget1, 0, 0, "Try0", IndexArrowL )
    AddItem(#ListIconGadget1, CountGadgetItems(#ListIconGadget1), 0, "Try1", IndexCroixB)
    AddItem(#ListIconGadget1, CountGadgetItems(#ListIconGadget1), 0, "Try2", IndexArrowR)
    AddItem(#ListIconGadget1, CountGadgetItems(#ListIconGadget1), 0, "Try3", IndexPBIco)
    AddItem(#ListIconGadget1, CountGadgetItems(#ListIconGadget1), 0, "Try4", IndexNumber4Ico)

    ; add Subitems
    For i = 0 To 4
      AddItem(#ListIconGadget1, i, 1, "SubItem" + Str(i), 0)
    Next

    Repeat
      Select WaitWindowEvent()
        Case #PB_Event_CloseWindow
          ImageList_Destroy_(Hwnd_ListSmall)
          End
      EndSelect
    ForEver

  EndIf
EndIf
End


